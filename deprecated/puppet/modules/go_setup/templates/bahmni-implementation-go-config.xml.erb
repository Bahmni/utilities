              <exec command="cp">
                <arg>bahmni-environment/scripts/ci/bahmni_deploy.sh</arg>
                <arg>deployables</arg>
                <runif status="passed" />
              </exec>
              <exec command="/bin/sh">
                <arg>bahmni-environment/scripts/ci/create_all_modules.sh</arg>
                <arg>#{implementationName}</arg>
                <runif status="passed" />
              </exec>
            </tasks>
            <resources>
